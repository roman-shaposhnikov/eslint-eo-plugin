{
  "name": "eslint-eo-plugin",
  "version": "0.0.0",
  "license": "ISC",
  "type": "module",
  "author": "Roman Shaposhnikov <roman_shaposhnikov_pub@mail.ru>",
  "description": "ESLint plugin for stick to the theory of Elegant Objects",
  "scripts": {
    "init": "./config/init.sh",
    "test": "npm run test:types && npm run test:run",
    "test:types": "tsc --noEmit",
    "test:run": "vitest run --config ./config/test/vitest.config.ts",
    "test:watch": "vitest watch --config ./config/test/vitest.config.ts",
    "build": "./config/build/build.sh"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "homepage": "https://github.com/roman-shaposhnikov/eslint-eo-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/roman-shaposhnikov/eslint-eo-plugin"
  },
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "require": "./_cjs/index.js",
      "import": "./_esm/index.js",
      "default": "./_esm/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "@typescript-eslint/rule-tester": "^8.39.1",
    "@typescript-eslint/utils": "^8.34.0",
    "eslint": "^9.28.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
